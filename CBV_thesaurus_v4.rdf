<rdf:Description rdf:about="https://data.wto.org/authority/thesaurus/concept_002_003">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:seeAlso rdf:resource="http://www.sice.oas.org/Dictionary/SV_e.asp#231"/>
