	<skos:prefLabel xml:lang="es">anulación o menoscabo</skos:prefLabel>
	<skos:altLabel xml:lang="en">nullify or impair</skos:altLabel>
	<skos:altLabel xml:lang="fr">annuler ou compromettre</skos:altLabel>
