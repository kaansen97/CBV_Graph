	<skos:prefLabel xml:lang="fr">zone d'échanges préférentiels</skos:prefLabel>
	<skos:prefLabel xml:lang="es">zona de comercio preferencial</skos:prefLabel>
	<skos:altLabel xml:lang="es">zona comercial preferencial.</skos:altLabel>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">330</skos:notation>
