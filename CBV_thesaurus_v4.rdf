	<skos:prefLabel xml:lang="fr">droits compensateurs</skos:prefLabel>
	<skos:prefLabel xml:lang="es">derechos compensatorios</skos:prefLabel>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">46</skos:notation>
