<rdf:Description rdf:about="https://data.wto.org/authority/thesaurus/concept_002_096">
	<rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
	<rdfs:seeAlso>WTO Legal texts (Article VI of GATT 1994)</rdfs:seeAlso>
	<skos:definition xml:lang="en">To determine the existence, degree and effect of any alleged dumping shall be initiated upon a written application by or on behalf of the domestic industry.</skos:definition>
	<skos:inScheme rdf:resource="https://data.wto.org/authority/thesaurus"/>
	<skos:inScheme rdf:resource="https://data.wto.org/authority/thesaurus/conceptscheme_2"/>
