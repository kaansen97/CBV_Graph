	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">492</skos:notation>
	<skos:broader rdf:resource="https://data.wto.org/authority/thesaurus/concept_001_164"/>
	<skos:related rdf:resource="https://data.wto.org/authority/thesaurus/concept_001_059"/>
