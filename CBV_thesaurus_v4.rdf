	<skos:inScheme rdf:resource="https://data.wto.org/authority/thesaurus/conceptscheme_2"/>
	<skos:prefLabel xml:lang="en">anti-dumping measures</skos:prefLabel>
	<skos:prefLabel xml:lang="fr">mesures antidumping</skos:prefLabel>
	<skos:prefLabel xml:lang="es">medidas antidumping</skos:prefLabel>
	<skos:altLabel xml:lang="en">anti-dumping</skos:altLabel>
	<skos:altLabel xml:lang="fr">antidumping</skos:altLabel>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">80</skos:notation>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">45</skos:notation>
	<skos:broader rdf:resource="https://data.wto.org/authority/thesaurus/concept_002_095"/>
	<skos:narrower rdf:resource="https://data.wto.org/authority/thesaurus/concept_002_098"/>
	<skos:related rdf:resource="https://data.wto.org/authority/thesaurus/concept_002_053"/>
