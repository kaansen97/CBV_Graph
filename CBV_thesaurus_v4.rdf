	<skos:prefLabel xml:lang="fr">accès aux marchés</skos:prefLabel>
	<skos:prefLabel xml:lang="es">acceso a los mercados</skos:prefLabel>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">18</skos:notation>
