	<skos:altLabel xml:lang="en">TBT</skos:altLabel>
	<skos:altLabel xml:lang="fr">OTC</skos:altLabel>
	<skos:altLabel xml:lang="es">OTC</skos:altLabel>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">1555</skos:notation>
	<skos:notation rdf:datatype="https://data.wto.org/authority/notation-types/dol-keyword">32</skos:notation>
